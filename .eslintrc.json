{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 13,
        "sourceType": "module"
    },
    "rules": {
        "no-console": "warn", 
        "no-use-before-define": "warn", 
        "spaced-comment": "warn",
        "linebreak-style": "off"
    }
}
